<script>
	window.getCurrServerTime = (function(s, c) {return function(){return +new Date()+s-c;}})(1601957647034, +new Date());
	window.__enableReport = true;
	window.lifeId = "r1Pf1dK8v";
	window.lifeStart = getCurrServerTime();
</script>
<script src="https://cloudcache.tencent-cloud.com/qcloud/insight/insight-client.985ac73c2.js"></script>
<script>
	try {
		if (typeof window.Insight === 'object') {
			Insight.install({
				app: 'qcmain',
				lid: window.lifeId,
				born: window.lifeStart,
				time: window.getCurrServerTime,
				shouldReportEvent: function (payload) {
					if (payload) {
						//  神策上报触发 cgi-error 和 xhr-error 忽略掉
						if (
							(payload.event == 'xhr-error' || payload.event == 'cgi-error')
							&& payload.hostname
							&& payload.hostname.indexOf('sensors.cloud.tencent.com') == 0
						) {
							return false;
						}
					}
					return window.__enableReport && document.cookie.search('disableInsight=1') === -1;
				}
			});
		}
	} catch(e) {
		console.warn('官网监控初始化失败');
	}
</script>
